<!-- ユーザーの詳細 -->
<div>
  <h3><%= @user.name %></h3>
  <%= image_tag @user.get_profile_image(100,100) %>
</div>

<!-- ユーザーの投稿一覧 -->

<% @books.each do |post_image| %>
<%= form_with model: @book do |f| %>
    <h4>Title</h4>
      <%= f.text_field :title %>
    <h4>Body</h4>
      <%= f.text_area :body %>
    <div>
      <%= f.submit 'Create Book' %>
    </div>
<% end %>

